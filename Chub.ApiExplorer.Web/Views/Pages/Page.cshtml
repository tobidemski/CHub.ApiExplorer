@model Page


<div class="card border-secondary mb-3 mt-3">
    <div class="card-header"><b>@(string.IsNullOrEmpty(this.Model.Title) ? this.Model.Name : this.Model.Title)</b></div>
    <!--<div class="card-body">
       <h4 class="card-title"></h4>
    </div>-->
    <ul class="list-group list-group-flush">
        <li class="list-group-item">Name: @this.Model.Name</li>
        <li class="list-group-item">Path: @this.Model.Path</li>
        <li class="list-group-item">Identifier: @this.Model.Identifier</li>
        <li class="list-group-item">Is Title Visible: <input class="form-check-input" type="checkbox" disabled @(this.Model.IsTitleVisible ? "checked" : "") /></li>
        <li class="list-group-item">Url: <a target="_blank" href="@this.Model.Url">Page Link</a></li>
    </ul>
</div>